{
  "name": "dummy-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/dummy-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/dummy-app",
        "index": "apps/dummy-app/src/index.html",
        "main": "apps/dummy-app/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/dummy-app/tsconfig.app.json",
        "stylePreprocessorOptions": {
          "includePaths": [
            "libs/shared/ui-theme/src/lib/styles/index.scss",
            "node_modules/bootstrap/scss"
          ]
        }
      }
    }
  }
}
