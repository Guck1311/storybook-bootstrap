{
  "name": "debug-core",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "author": "Marcel Burgstaller",
  "workspaces": [
    "libs/**"
  ],
  "scripts": {
    "build": "nx run-many --target=build --projects=ui-theme,ui-components",
    "build:storybook": "nx run ui-components:build-storybook",
    "build:ui-components": "nx build ui-components",
    "build:ui-theme": "nx build ui-theme",
    "format": "npx sort-package-json && prettier --write \"**/*.{ts,html,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,html,scss,json,md}\"",
    "generate:component": "tsx generate-component.ts",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "prepare": "husky install",
    "storybook": "nx run ui-components:storybook",
    "test": "npm run test:coverage && npm run test:cypress && npm run test:jest && npm run test:storybook",
    "test:coverage": "nx test-jest ui-components --codeCoverage",
    "test:cypress": "nx component-test ui-components",
    "test:cypress:gui": "nx run ui-components:component-test --watch",
    "test:jest": "nx test-jest ui-components",
    "type-check": "tsc --noEmit -p tsconfig.base.json"
  },
  "lint-staged": {
    "*.{ts,html,scss}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@angular/animations": "~20.0.0",
    "@angular/common": "~20.0.0",
    "@angular/compiler": "~20.0.0",
    "@angular/core": "~20.0.0",
    "@angular/forms": "~20.0.0",
    "@angular/platform-browser": "~20.0.0",
    "@angular/platform-browser-dynamic": "~20.0.0",
    "@angular/router": "~20.0.0",
    "@awesome.me/kit-cd3e3142ca": "^1.0.10",
    "@fortawesome/angular-fontawesome": "^2.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/pro-solid-svg-icons": "^6.7.2",
    "bootstrap": "^5.3.6",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.0.0",
    "@angular-devkit/core": "~20.0.0",
    "@angular-devkit/schematics": "~20.0.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular-eslint/eslint-plugin-template": "^20.0.0",
    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/cli": "~20.0.0",
    "@angular/compiler-cli": "~20.0.0",
    "@angular/language-service": "~20.0.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/angular": "^3.0.0",
    "@cypress/webpack-dev-server": "^4.1.0",
    "@eslint/js": "^9.8.0",
    "@html-eslint/eslint-plugin": "^0.41.0",
    "@nx/angular": "^21.3.11",
    "@nx/cypress": "21.2.1",
    "@nx/eslint": "21.2.1",
    "@nx/eslint-plugin": "21.2.1",
    "@nx/jest": "21.2.1",
    "@nx/js": "21.2.1",
    "@nx/storybook": "21.2.1",
    "@nx/web": "21.2.1",
    "@nx/workspace": "21.2.1",
    "@schematics/angular": "~20.0.0",
    "@storybook/addon-styling-webpack": "2.0.0",
    "@storybook/angular": "9.0.14",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.14",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.29.0",
    "angular-eslint": "^20.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cypress": "^14.5.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
